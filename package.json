{
  "name": "TMessage",
  "version": "0.0.1",
  "description": "TMessage",
  "productName": "TMessage",
  "author": "khanhdn <ngockhanh97nd@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env ENVIRONMENT=development quasar dev",
    "build:dev": "cross-env ENVIRONMENT=development quasar build",
    "dev:ssr": "quasar dev -m ssr",
    "build": "quasar build",
    "build:prod": "cross-env ENVIRONMENT=production quasar build",
    "deploy": "cd docker/ && docker system prune -a -f && docker-compose up -d --build",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .eslintignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .eslintignore",
    "lint:prettier": "prettier --check . ",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.21.1",
    "chart.js": "^4.2.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "click-outside-vue3": "^4.0.1",
    "emoji-mart-vue-fast": "^10.0.1",
    "firebase": "^9.17.1",
    "floating-vue": "^2.0.0-beta.20",
    "lodash": "^4.17.21",
    "pinia": "^2.0.11",
    "qs": "^6.11.1",
    "quasar": "^2.6.0",
    "secure-ls": "^1.2.6",
    "socket.io-client": "^4.6.1",
    "vue": "^3.0.0",
    "vue-contenteditable": "^3.0.4",
    "vue-cropper": "^0.5.11",
    "vue-i18n": "^9.3.0-beta.16",
    "vue-mention": "^2.0.0-alpha.3",
    "vue-router": "^4.0.0",
    "vue-zoomer": "^0.4.0-beta.2",
    "vue3-scroll-picker": "^0.1.15",
    "vue3-virtual-scroller": "^0.2.3",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^7.0.0",
    "@quasar/app-vite": "^1.0.0",
    "@quasar/quasar-app-extension-qcalendar": "^4.0.0-beta.15",
    "@types/lodash": "^4.14.195",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "mark.js": "^8.11.1",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "typescript": "^4.5.4",
    "vue-eslint-parser": "^9.1.0"
  },
  "browserslist": [
    "ie >= 11",
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
